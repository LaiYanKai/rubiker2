    <node pkg="rubiker" type="tacho" name="tachoFW" output="screen" args="$(env MotorFW)"/>
    <node pkg="rubiker" type="tacho" name="tachoFH" output="screen" args="$(env MotorFH)"/>
    <node pkg="rubiker" type="tacho" name="tachoBW" output="screen" args="$(env MotorBW)"/>
    <node pkg="rubiker" type="tacho" name="tachoBH" output="screen" args="$(env MotorBH)"/>
</launch>
