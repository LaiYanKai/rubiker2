    <node pkg="rubiker" type="pid" name="pidFW" output="screen" args="$(env MotorFW)"/>
    <node pkg="rubiker" type="pid" name="pidFH" output="screen" args="$(env MotorFH)"/>
    <node pkg="rubiker" type="pid" name="pidBW" output="screen" args="$(env MotorBW)"/>
    <node pkg="rubiker" type="pid" name="pidBH" output="screen" args="$(env MotorBH)"/>
